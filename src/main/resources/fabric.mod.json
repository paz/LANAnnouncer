{
	"schemaVersion": 1,
	"id": "lanannouncer",
	"version": "${version}",
	"name": "LANAnnouncer",
	"description": "Announces the server to the LAN using multicast UDP.",
	"authors": [
		"Gunnar Guðvarðarson"
	],
	"contact": {
		"sources": "https://github.com/meh-is/LANAnnouncer",
		"issues": "https://github.com/meh-is/LANAnnouncer/issues"
	},
	"license": "MIT",
	"icon": "assets/lanannouncer/icon.png",
	"environment": "server",
	"entrypoints": {
		"main": [
			"is.meh.minecraft.lan_announcer.LANAnnouncer"
		]
	},
	"depends": {
    "fabric": "${fabric_version_range}",
    "minecraft": ">=${minecraft_version_min} <${minecraft_version_max}",
		"fabricloader": "${fabric_loader_version_range}",
		"java": ">=17",
		"fabric-api": "*"
	}
